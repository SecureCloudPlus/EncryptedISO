<Window x:Class="ExtractISO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExtractISO"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="271">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="13*"/>
            <ColumnDefinition Width="167*"/>
            <ColumnDefinition Width="20*"/>
            <ColumnDefinition Width="52*"/>
            <ColumnDefinition Width="19*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Content="Password" HorizontalAlignment="Left" Margin="9,23,0,0" VerticalAlignment="Top" Height="26" Width="60" Grid.Column="1"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="9,54,0,0" VerticalAlignment="Top" Width="220" Height="18" Grid.Column="1" Grid.ColumnSpan="3"/>
        <Button x:Name="button" Content="Button" Grid.Column="1" HorizontalAlignment="Left" Margin="65,95,0,0" VerticalAlignment="Top" Width="108" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
